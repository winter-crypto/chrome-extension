{
  "name": "Winter checkout",
  "description": "An easy way to checkout with credit card & bank account on Opensea!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "winterLogo.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.opensea.io/*"],
      "js": ["inject.js"]
    }
  ],
  "host_permissions": ["<all_urls>"]
}
